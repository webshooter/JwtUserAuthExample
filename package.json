{
  "name": "JwtUserAuthExample",
  "version": "1.0.0",
  "description": "node/express-mongo-jwt based user auth example project",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=development nodemon --exec babel-node src/index.js",
    "test": "jest",
    "sass": "sass --load-path=./node_modules/foundation-sites/assets --style=expanded ./src/scss:./public/css",
    "sass:compressed": "sass ./src/scss:./public/css --style=compressed",
    "prefix": "npm run sass && postcss ./public/css/app.css --use=autoprefixer --output=./public/css/app.css",
    "sass:watch": "onchange './src/scss/**/*.scss' -- npm run prefix"
  },
  "author": "Randall Nickerson",
  "license": "ISC",
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/__test__/setup.js"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.5",
    "autoprefixer": "^9.7.3",
    "babel-jest": "^24.9.0",
    "browser-sync": "^2.26.7",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "faker": "^4.1.0",
    "jest-cli": "^24.9.0",
    "mongodb-memory-server": "^6.0.2",
    "nodemon": "^2.0.1",
    "onchange": "^6.1.0",
    "postcss": "^7.0.25",
    "postcss-cli": "^6.1.3",
    "sass": "^1.23.7"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "foundation-sites": "^6.6.1",
    "generate-password": "^1.4.2",
    "ip-regex": "^4.1.0",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "mongodb": "^3.3.5",
    "morgan": "^1.9.1",
    "motion-ui": "^2.0.3",
    "pug": "^2.0.4",
    "uuid": "^3.3.3",
    "uuid-validate": "0.0.3",
    "what-input": "^5.2.6",
    "winston": "^3.2.1"
  }
}
